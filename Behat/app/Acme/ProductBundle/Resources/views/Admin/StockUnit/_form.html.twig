<ul class="nav nav-tabs">
    <li class="active">
        <a href="#stock-unit-general" data-toggle="tab">{{ 'ekyna_core.field.general'|trans }}</a>
    </li>
</ul>
<div class="panel-body form-body">
    {{ bootstrap_set_style('horizontal') }}
    {{ bootstrap_set_col_size('md') }}
    {{ form_errors(form) }}
    <div class="tab-content">
        <div class="tab-pane active" id="stock-unit-general">
            <div class="row">
                <div class="col-md-6">
                    {{ form_row(form.netPrice, {'label_col': 4, 'widget_col': 8}) }}
                </div>
                <div class="col-md-6">
                    {{ form_row(form.geocode, {'label_col': 4, 'widget_col': 8}) }}
                </div>
            </div>
            <div class="row">
                <div class="col-md-6">
                    {{ form_row(form.orderedQuantity, {'label_col': 4, 'widget_col': 8}) }}
                </div>
                <div class="col-md-6">
                    {{ form_row(form.deliveredQuantity, {'label_col': 4, 'widget_col': 8}) }}
                </div>
            </div>
            <div class="row">
                <div class="col-md-6">
                    {{ form_row(form.estimatedDateOfArrival, {'label_col': 4, 'widget_col': 8}) }}
                </div>
                <div class="col-md-6">
                    {{ form_row(form.shippedQuantity, {'label_col': 4, 'widget_col': 8}) }}
                </div>
            </div>
        </div>
    </div>
    {{ form_rest(form) }}
</div>
