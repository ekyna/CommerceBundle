{% extends '@EkynaAdmin/Entity/Crud/read.html.twig' %}

{% block actions %}{% endblock %}

{% block main %}
<div class="actions">
    {{ admin_resource_btn(supplierDelivery, 'Ekyna\\Bundle\\AdminBundle\\Action\\UpdateAction') }}
    {{ admin_resource_btn(supplierDelivery, 'Ekyna\\Bundle\\AdminBundle\\Action\\DeleteAction') }}
</div>
<div class="show show-with-tabs">
    <ul class="nav nav-tabs" data-tab-key="{{ context.config.id }}">
        <li class="active">
            <a href="#supplier-delivery-general" id="toggle-general" data-toggle="tab">
                {{ 'field.general'|trans({}, 'EkynaUi') }}
            </a>
        </li>
    </ul>
    <div class="show-body">
        <div class="tab-content">
            <div class="tab-pane active" id="supplier-delivery-general">
                {#{{ show_row(supplierDelivery.street, 'text', {'label': t('field.street', [], 'EkynaUi')}) }}#}
            </div>
        </div>
    </div>
</div>
{% endblock %}
