<!doctype html>
<html>
<head>
    <style type="text/css">
        html,
        body {
            width: 100%;
            height: 100%;
            margin :0;
            padding: 0;
        }

        img {
            width: 100%;
            height: auto;
        }

        div.a6 {
            box-sizing: border-box;
            float: left;
            width: 50%;
            height: 50%;
            padding: 1cm;
        }
    </style>
</head>
<body>

    {% for data in labels %}
        <div class="a6">
            <img src="data:image/gif;base64,{{ data }}">
        </div>
    {% endfor %}

</body>
</html>
