{% extends '@EkynaAdmin/Entity/sided.html.twig' %}

{% block page_title %}{{ (context.config.transPrefix ~ '.button.import')|trans({}, context.config.transDomain)|raw }}{% endblock page_title %}

{% block title %}
    <h1>{{ (context.config.transPrefix ~ '.button.import')|trans({}, context.config.transDomain)|raw }}</h1>
{% endblock title %}

{% block actions %}{% endblock actions %}

{% block main %}
    {{ form_start(form) }}
    <div class="form-body">
        {{ bootstrap_set_style('horizontal') }}
        {{ bootstrap_set_col_size('md') }}
        {{ bootstrap_set_sizing('sm') }}
        {{ form_errors(form) }}
        {{ form_widget(form) }}
        {{ form_rest(form) }}
    </div>
    {{ form_end(form) }}
{% endblock main %}
