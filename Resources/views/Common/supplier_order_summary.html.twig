{% spaceless %}
<table class="table table-condensed">
<thead>
    <tr>
        <th>{{ 'ekyna_core.field.designation'|trans }}</th>
        <th>{{ 'ekyna_commerce.supplier_order_item.field.net_price'|trans }}</th>
        <th>{{ 'ekyna_commerce.stock_unit.field.ordered_quantity'|trans }}</th>
        <th>{{ 'ekyna_commerce.stock_unit.field.received_quantity'|trans }}</th>
    </tr>
</thead>
<tbody>
{% for item in items %}
    <tr>
        <td>{{ item.designation }}</td>
        <td>{{ item.net_price }}</td>
        <td>{{ item.ordered }}</td>
        <td>{{ item.received }}</td>
    </tr>
{% endfor %}
</tbody>
</table>
{% endspaceless %}